<!-- PROFILE PAGE TOP WRAPPER -->
<section class="profile-page-main-wrapper">
  <div class="profile-page-user-photo-wrapper">
    <ul>
      <li class="profile-page-user-photo-list"><img class="profile-page-user-photo" src="http://www.john-michael.net/wp-content/uploads/2011/11/Teddy-Roosevelt.jpg"></li>
    </ul>
  </div>
  <div class="user-profile-information-wrapper">
    <ul class = "user-profile-information">
      <li class="profile-page-user-name"><span class="profile-page-user-label">Name:</span><span class="user-profile-unique-label">&nbsp&nbspTheodore Roosevelt</span></li>
      <li class="profile-page-user-grades"><span class="profile-page-user-label">Grades I Teach:</span><span class="user-profile-unique-label">&nbsp&nbspKindergarten</span></li>
      <li class="profile-page-user-school"><span class="profile-page-user-label">My School Name:</span><span class="user-profile-unique-label">&nbsp&nbspTheodore Roosevelt Regional Awesomeness Elementary School</span></li>
      <li class="profile-page-user-city"><span class="profile-page-user-label">School City:</span><span class="user-profile-unique-label">&nbsp&nbspWalla Walla</span></li>
      <li class="profile-page-user-state"><span class="profile-page-user-label">School State:</span><span class="user-profile-unique-label">&nbsp&nbspWashington</span></li>
    </ul>
  </div>

  <div class="profile-page-top-wrapper">
  <div class="expander-profile">
      <a href="javascript:void(0)" id="js-expander-trigger" class="expander-profile-trigger expander-hidden">Following</a>
      <div id="js-expander-content" class="expander-content">

      <div class="user-followers-wrapper-top">
        <h4 class="followers-title">Followers</h4>
        <ul class = "user-followers-containter">
          <li class="followers-name">Joseph Stalin</li>
          <li class="followers-name">Cher</li>
          <li class="followers-name">Taylor Swift</li>
        </ul>
      </div>


      <div class="user-followers-wrapper-bottom">
        <h4 class="following-title">Following</h4>
        <ul class ="user-followers-containter">
          <li class="following-name">Joseph Stalin</li>
          <li class="following-name">Cher</li>
          <li class="following-name">Taylor Swift</li>
        </ul>
      </div>

      </div>
    </div>
  </div>
</section>



