<section class="profile-wrapper">

  <aside class="user-profile-photo-wrapper">
    <%= image_tag @user.avatar, class: "user-profile-photo" %>
  </aside>

  <aside class="user-profile-information-wrapper-left">
    <ul class = "user-profile-information-categories"
      <li>Name:</li>
      <li>Grade(s):</li>
      <li>School Name:</li>
      <li>City:</li>
      <li>State:</li>
    </ul>
  </aside>

  <aside class="user-profile-information-wrapper-right">
    <ul class = "user-profile-unique-information">
      <li><%= @user.name %></li>
      <li><%= @user.grade %></li>
      <li><%= @user.school %></li>
      <li><%= @user.city %></li>
      <li><%= @user.state %></li>
    </ul>
  </aside>
</section>
